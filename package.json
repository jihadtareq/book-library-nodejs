{
  "private": true,
  "scripts": {
    "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "npm run development -- --watch",
    "watch-poll": "npm run watch -- --watch-poll",
    "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "start": "node server.js",
    "dev": "nodemon server.js",
    "db:migrate": "sequelize db:migrate",
    "db:migrate:production": "sequelize db:migrate --env production",
    "db:drop": "sequelize db:drop",
    "db:create": "sequelize db:create",
    "db:update": "npm run db:drop && npm run db:create && npm run db:migrate"
  },
  "devDependencies": {
    "axios": "^0.18.1",
    "bootstrap": "^4.1.0",
    "cross-env": "^5.1",
    "jquery": "^3.2",
    "laravel-mix": "^4.0.7",
    "lodash": "^4.17.5",
    "popper.js": "^1.12",
    "resolve-url-loader": "^2.3.1",
    "sass": "^1.15.2",
    "sass-loader": "^7.1.0",
    "vue": "^2.5.17"
  },
  "dependencies": {
    "date-and-time": "^3.0.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "ioredis": "^4.28.5",
    "laravel-echo-server": "^1.6.3",
    "moment": "^2.29.4",
    "mysql": "^2.18.1",
    "mysql2": "^3.6.2",
    "nodemon": "^3.0.1",
    "redis": "^4.6.5",
    "sequelize": "^6.33.0",
    "socket.io": "^4.6.1",
    "socket.io-redis": "^6.1.1",
    "twilio": "^4.19.0",
    "uuid": "^9.0.0"
  },
  "name": "book-library",
  "version": "1.0.0",
  "description": "Library Management System",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jihadtareq/book-library-nodejs.git"
